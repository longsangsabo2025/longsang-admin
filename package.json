{
  "name": "longsang-admin",
  "description": "Master Admin Dashboard - Manage all LongSang projects, automations, and integrations",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "electron/main.cjs",
  "author": "LongSang Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/longsang/long-sang-forge.git"
  },
  "bugs": {
    "url": "https://github.com/longsang/long-sang-forge/issues"
  },
  "homepage": "https://github.com/longsang/long-sang-forge#readme",
  "keywords": [
    "automation",
    "ai",
    "social-media",
    "content-generation",
    "workflow",
    "react",
    "typescript",
    "vite",
    "supabase",
    "electron",
    "desktop"
  ],
  "scripts": {
    "dev": "concurrently -n \"VITE,API\" -c \"cyan,green\" \"npm run dev:frontend\" \"npm run dev:api\"",
    "dev:frontend": "vite",
    "dev:kb-api": "node kb-api-server.mjs",
    "dev:api": "cd api && node server.js",
    "start": "npm run dev",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "desktop": "electron .",
    "desktop:dev": "cross-env NODE_ENV=development electron .",
    "desktop:build": "npm run build && electron-builder",
    "desktop:build:win": "npm run build && electron-builder --win",
    "desktop:build:mac": "npm run build && electron-builder --mac",
    "desktop:build:linux": "npm run build && electron-builder --linux",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "test:all": "node scripts/run-all-tests.js",
    "test:unit": "vitest run src/**/*.test.ts",
    "test:integration": "vitest run tests/integration/**/*.test.ts",
    "test:e2e": "vitest run tests/e2e/**/*.test.ts",
    "test:marketplace": "node scripts/test-marketplace-e2e.mjs",
    "test:system": "node scripts/test-system.js",
    "test:fix": "node scripts/test-system.js --fix",
    "setup:phase1": "node scripts/setup-phase1-auto.js",
    "migrate:bug-system": "node scripts/auto-migrate-bug-system.mjs",
    "test:bug-system": "node scripts/test-bug-system.mjs",
    "setup:bug-system": "node scripts/setup-bug-system-auto.mjs",
    "fix:auto": "node scripts/auto-fix-errors-from-db.mjs",
    "fix:loop": "node scripts/auto-fix-errors-from-db.mjs",
    "index:all": "node scripts/index-all-data.js",
    "test:phase1": "node scripts/test-phase1-endpoints.js",
    "debug:activate": "node scripts/debug-activate.mjs",
    "deploy:all": "powershell -ExecutionPolicy Bypass -File ./scripts/deploy-all.ps1",
    "deploy:db": "supabase db push",
    "deploy:functions": "supabase functions deploy trigger-content-writer && supabase functions deploy send-scheduled-emails && supabase functions deploy publish-social-posts && supabase functions deploy generate-sitemap",
    "supabase:link": "supabase link --project-ref diexsbzqwsbpilsymnfb",
    "setup:hooks": "node scripts/setup-husky.js",
    "supabase:status": "supabase functions list && supabase secrets list",
    "setup": "npm install && npm run supabase:link && npm run deploy:db",
    "workflows": "powershell -ExecutionPolicy Bypass -File ./scripts/start-n8n.ps1",
    "seo:analyze": "node scripts/seo-analyzer.mjs",
    "seo:monitor": "node scripts/seo-performance-monitor.mjs",
    "seo:generate-sitemap": "node scripts/generate-sitemap.mjs",
    "seo:audit": "node scripts/seo-audit.mjs",
    "seo:keywords": "node scripts/keyword-research.mjs",
    "seo:score": "node scripts/calculate-seo-scores.mjs",
    "seo:sync-gsc": "supabase functions invoke search-console-sync --method POST",
    "seo:full-report": "npm run seo:analyze && npm run seo:monitor && echo 'SEO Analysis Complete!'",
    "admin:set-role": "tsx scripts/set-admin.ts",
    "test:mcp-supabase": "node scripts/test-mcp-supabase.js",
    "prepare": "husky"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
    "@google-analytics/data": "^5.2.1",
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-context-menu": "^2.2.15",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@sentry/react": "^10.25.0",
    "@sentry/vite-plugin": "^4.6.0",
    "@supabase/supabase-js": "^2.75.0",
    "@tanstack/react-query": "^5.83.0",
    "@types/nodemailer": "^7.0.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "ai": "^5.0.102",
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^17.2.3",
    "electron-updater": "^6.6.2",
    "embla-carousel-react": "^8.6.0",
    "express": "^5.1.0",
    "framer-motion": "^12.23.24",
    "google-ads-api": "^21.0.1",
    "googleapis": "^165.0.0",
    "highlight.js": "^11.11.1",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "input-otp": "^1.4.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.554.0",
    "next-themes": "^0.3.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.11",
    "openai": "^6.9.1",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.61.1",
    "react-i18next": "^16.0.1",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^2.1.9",
    "react-router-dom": "^6.30.1",
    "react-syntax-highlighter": "^16.1.0",
    "reactflow": "^11.11.4",
    "recharts": "^2.15.4",
    "redis": "^5.10.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "sonner": "^1.7.4",
    "stripe": "^19.3.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.9",
    "web-vitals": "^5.1.0",
    "winston": "^3.18.3",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.19.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "@vitest/ui": "^4.0.8",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "lint-staged": "^16.2.7",
    "lovable-tagger": "^1.1.11",
    "markdownlint-cli2": "^0.19.0",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "server-destroy": "^1.0.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^5.4.21",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^4.0.8",
    "wait-on": "^9.0.3",
    "workbox-window": "^7.3.0"
  },
  "build": {
    "appId": "com.longsang.admin",
    "productName": "LongSang Admin",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "!node_modules/**/*"
    ],
    "extraResources": [
      {
        "from": ".env",
        "to": ".env"
      }
    ],
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "electron/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "electron/icon.icns"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "icon": "electron/icon.png"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --max-warnings=0"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  }
}
